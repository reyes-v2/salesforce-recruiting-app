<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Max_pay</fullName>
    <active>true</active>
    <description>Max Pax should not be greater than 1M</description>
    <errorConditionFormula>Max_Pay__c  &gt; 1000000</errorConditionFormula>
    <errorDisplayField>Max_Pay__c</errorDisplayField>
    <errorMessage>Max Pay cannot be greater than 1 million</errorMessage>
</ValidationRule>
